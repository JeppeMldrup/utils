super+w
	firefox &
super+s
	st ffm
XF86AudioRaiseVolume
	amixer -c 1 sset Master 2db+; /home/jeppe/scripts/resetbar.sh
XF86AudioLowerVolume
	amixer -c 1 sset Master 2db-; /home/jeppe/scripts/resetbar.sh
XF86AudioMute
	amixer -D pulse set Master 1+ toggle; /home/jeppe/scripts/resetbar.sh
XF86MonBrightnessUp
	xbacklight -inc 10
XF86MonBrightnessDown
	xbacklight -dec 10

super+shift+Return
    st &
super+p
    dmenu_run -fn SourceCodePro:size=12.5

super+{h,j,k,l}
    bspc node {west,south,north,east} -f; /home/jeppe/scripts/resetbar.sh
super+shift+{h,j,k,l}
    bspc node {west,south,north,east} -s focused
super+alt+{h,j,k,l}
    bspc node focused -z {left -20 0,bottom 0 +20,top 0 -20,right +20 0}
super+ctrl+{h,j,k,l}
    bspc node -p {west,south,north,east}
super+{f,t}
    bspc node focused -t {fullscreen,tiled}

super+space
    bspc desktop -l next; /home/jeppe/scripts/resetbar.sh
    
super+Tab
    bspc desktop last -f; /home/jeppe/scripts/resetbar.sh
super+{1,2,3,4,5,6,7,8,9}
    bspc desktop {^1,^2,^3,^4,^5,^6,^7,^8,^9} -f; /home/jeppe/scripts/resetbar.sh
super+shift+{1,2,3,4,5,6,7,8,9}
    bspc node focused -d {^1,^2,^3,^4,^5,^6,^7,^8,^9}; /home/jeppe/scripts/resetbar.sh

super+shift+c
    bspc node focused -k
super+shift+q
    bspc quit

